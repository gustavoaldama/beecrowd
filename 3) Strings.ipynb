    "# 3300 - Numeros Ma Sorte Regarregados\n",
    "\n",
    "n = int(input())\n",
    "s = [\"es de Mala Suerte\",\"NO es de Mala Suerte\"]\n",
    "x = [int(a) for a in str(n)]\n",
    "i = 0\n",
    "while i<len(x)-1:\n",
    "    if x[i]==1 and x[i+1]==3:\n",
    "        print(n,s[0])\n",
    "        break\n",
    "    i=i+1\n",
    "if i==len(x)-1:\n",
    "        print(n,s[1])"
