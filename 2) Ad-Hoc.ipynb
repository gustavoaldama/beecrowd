   "source": [
    "# 1030 - A Lenda de flavius Josefo\n",
    "\n",
    "#(x)\n",
    "\n",
    "q = int(input())\n",
    "c=0\n",
    "for _ in range(q):\n",
    "    c=c+1\n",
    "    n, k = map(int, input().split())\n",
    "    l = list(range(n))\n",
    "    k -= 1\n",
    "    i = k\n",
    "    while len(l) > 1:\n",
    "        l.pop(i)\n",
    "        i = (i + k) % len(l)\n",
    "    print('case {0}: {1}'.format(c,l[0]))"
